{
  "name": "eUser",
  "plural": "eUsers",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "contact_data": {
      "type": "object"
    },
    "free_trial": {
      "type": "boolean"
    },
    "position": {
      "type": "string"
    },
    "role_id": {
      "type": "number",
      "required": true,
      "default": 2
    },
    "created_at": {
      "type": "date",
      "required": true
    },
    "updated_at": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "company": {
      "type": "belongsTo",
      "model": "Company",
      "foreignKey": "company_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
